(defun uniq (&rest inds)
  ;; (read-from-string (apply #'mkstr inds))
  (loop for i from 0 to (- (length inds) 1) 
     summing (* (expt 10000 (- (length inds) i 1)) (nth i inds)) into result 
     ;; summing (nth i inds) into result 
     finally (return result)))

