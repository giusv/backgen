(indicatore test-sco1 (soggetto) (durata occorrenze)
  (cond ((ind-ge (cluster (sinistri soggetto (coinvolto-diretto t))
                      durata)
             occorrenze) (values t t))
        ((not (ind-ge (cluster (sinistri soggetto (coinvolto-diretto nil)) 
                           durata)
                  occorrenze)) (values nil t))
        (t (values nil nil))))

