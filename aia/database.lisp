  (cond ((ind-ge (cluster (sinistri soggetto (and (ind-ge (numero-lesi t) lesi)
                                              (coinvolto-diretto t)))
                      durata)
             occorrenze) (values t t))
        ((not (ind-ge (cluster (sinistri soggetto (and (ind-ge (numero-lesi nil) lesi)
                                              (coinvolto-diretto nil))) 
                           durata)
                  occorrenze)) (values nil t))
        (t (values nil nil))))

(indicatore test-sco4 (soggetto) (durata occorrenze ritardo)
  (cond ((ind-ge (cluster (sinistri soggetto (and (ind-ge (ind-minus (datadenuncia t) (dataaccadimento t)) (* ritardo 86400))
                                                  (coinvolto-diretto t)))
                          durata)
                 occorrenze) (values t t))
        ((not (ind-ge (cluster (sinistri soggetto (and (ind-ge (ind-minus (datadenuncia nil) (dataaccadimento nil)) (* ritardo 86400))
                                                       (coinvolto-diretto nil))) 
                               durata)
                      occorrenze)) (values nil t))
        (t (values nil nil))))



